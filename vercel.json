{
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@4.0.2"
    }
  },
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'",
      "npm_config_build_from_source": "false"
    }
  },
  "github": {
    "silent": true
  },
  "installCommand": "npm install --ignore-scripts"
}
